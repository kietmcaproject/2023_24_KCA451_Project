{
  "name": "spotify-client",
  "version": "0.2.3",
  "private": true,
  "dependencies": {
    "axios": "^1.7.2",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-drag-listview": "^0.2.1",
    "react-infinite-scroller": "^1.2.6",
    "react-player-controls": "^1.1.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.77.2"
  },
  "scripts": {
    "build-css": "sass --load-path=src src/:src/",
    "watch-css": "yarn run build-css && sass -w --load-path=src src/:src/",
    "start-js": "react-scripts --openssl-legacy-provider start",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts --openssl-legacy-provider build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
